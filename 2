using System;
using System.Net.Http;
using System.Threading.Tasks;

class MainClass
{
    static readonly HttpClient client = new HttpClient();

    static async Task Main(string[] args)
    {
        // Set the base URL of the API
        client.BaseAddress = new Uri("http://api.example.com");

        // Add any required headers or authentication credentials
        client.DefaultRequestHeaders.Authorization = new System.Net.Http.Headers.AuthenticationHeaderValue("Bearer", "YOUR_ACCESS_TOKEN");

        // Make a GET request to the API
        HttpResponseMessage response = await client.GetAsync("/users");

        // Read the response content as a string
        string responseBody = await response.Content.ReadAsStringAsync();

        Console.WriteLine(responseBody);
    }
}
